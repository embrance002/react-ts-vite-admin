{
  "name": "react-ts-vite-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preinstall": "yarn config set ignore-engines false && npx npm-only-allow@latest --PM yarn",
    "dev": "vite",
    "devonLine": "npx localtunnel --port 3301",
    "build:lint": "npm run lint:fix && vite build --mode production",
    "build:tsc": "tsc && vite build --mode production",
    "lint:fix": "eslint --fix --ext .js,.ts,.tsx ./src",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,ts,json,tsx,css,less,scss,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix \"**/*.{less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "tsc": "tsc --noEmit",
    "preview": "vite preview"
  },
  "dependencies": {
    "@ant-design/happy-work-theme": "^1.0.0",
    "ahooks": "^3.7.10",
    "antd": "^5.15.3",
    "axios": "^1.6.8",
    "classnames": "^2.5.1",
    "dayjs": "^1.11.10",
    "echarts": "^5.5.0",
    "js-cookie": "^3.0.5",
    "js-md5": "^0.8.3",
    "lodash-es": "^4.17.21",
    "mobx": "^6.12.0",
    "mobx-persist-store": "^1.1.4",
    "mobx-react-lite": "^4.0.6",
    "nprogress": "^0.2.0",
    "qs": "^6.12.0",
    "react": "^18.2.0",
    "react-activation": "^0.12.4",
    "react-dom": "^18.2.0",
    "react-intl": "^6.6.3",
    "react-transition-group": "^4.4.5",
    "screenfull": "^6.0.2",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.28",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-basic-ssl": "^1.1.0",
    "@vitejs/plugin-react": "^4.0.3",
    "autoprefixer": "^10.4.19",
    "cross-env": "^7.0.3",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "fontmin": "^1.0.1",
    "less": "^4.2.0",
    "lint-staged": "^15.2.2",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "react-router-dom": "^6.22.3",
    "rollup-plugin-visualizer": "^5.12.0",
    "stylelint": "^16.3.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-less": "^3.0.1",
    "typescript": "5.0.2",
    "vite": "^4.4.5",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-imagemin": "^0.6.1"
  },
  "resolutions": {
    "bin-wrapper": "npm:bin-wrapper-china"
  },
  "engines": {
    "node": "v18.x"
  }
}
